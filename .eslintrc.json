{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true,
        "jasmine": true,
        "codeceptjs": true
        
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-underscore-dangle": "off",
        "dot-notation": "off",
        "no-console": "off",
        "no-restricted-globals": "off",
        "class-methods-use-this": "off",
        "no-new": "off",
        "consistent-return": "off"
    }
}
